        <script>
            $(".dark_mode ").click(function() {
                const theme = localStorage.getItem('theme')
                const sun = '<i class="fa fa-sun "></i>'
                const moon = '<i class="fa fa-moon "></i>'
                if (theme == null || theme == "default") {
                    $(this).html(moon)
                    localStorage.setItem('theme', 'dark')
                    $('html').removeClass("default ")
                    $('html').addClass("dark ")
                    $(".theme-color").attr("content", "#161b22")
                } else {
                    $(this).html(sun)
                    localStorage.setItem('theme', 'default')
                    $('html').addClass("default")
                    $('html').removeClass("dark")
                    $(".theme-color").attr("content", "#991b1b")
                }
            })
            $.ripple(".rpl", {
                debug: false,
                on: 'mousedown',
                opacity: 0.3,
                color: "#a5dc86",
                multi: false,
                duration: 0.7,
                rate: function(pxPerSecond) {
                    return pxPerSecond;
                },
                easing: 'linear'
            })
            $.ripple(".rpl-h", {
                debug: false,
                on: 'mouseover',
                opacity: 0.3,
                color: "auto",
                multi: false,
                duration: 0.5,
                rate: function(pxPerSecond) {
                    return pxPerSecond;
                },
                easing: 'linear'
            })
        </script>
