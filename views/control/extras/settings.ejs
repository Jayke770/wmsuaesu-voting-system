<div class="settings fixed z-20 top-0 bg-darkBlue-primary/50 w-full h-full hidden justify-end items-center sm:items-end p-2 sm:p-0 transition-all">
    <div class="settings_main w-[400px] sm:w-full h-full sm:h-5/6 dark_border rounded-md sm:rounded-b-none shadow-md dark:bg-darkBlue-secondary" animate-in="animate__animated animate__fadeInUp ms-500" animate-out="animate__animated animate__fadeOutDown ms-500">
        <div class="w-full dark_border_b flex justify-start items-center py-2.5 px-3">
            <p class="text-xl font-medium sm:text-lg dark:text-gray-300">Settings</p>
        </div>
        <div class="settings_main_list container h-[calc(100%-49px)] p-2 overflow-y-auto transition-all">
            <div class="flex flex-col gap-3 py-2">
                <div class="flex flex-col gap-2">
                    <div class="animate__animated animate__fadeInUp ms-500 flex items-center px-1">
                        <span class="dark:text-gray-300 text-base">System Settings</span>
                    </div>
                    <div style="animation-delay: .250s;" class="update_sy animate__animated animate__fadeInUp ms-500 dark_border grid grid-cols-3 px-3 py-2 cursor-pointer rpl dark:bg-darkBlue-secondary rounded-lg shadow-lg">
                        <div class="flex flex-col col-span-2">
                            <p class="dark:text-gray-300 text-bluegray-900">School Year</p>
                            <p class="dark:text-gray-400 text-bluegray-500 text-sm ml-1 mt-1"><%- sy %></span>
                        </div>
                        <div class="flex items-center flex-row justify-end">
                            <i class="settings_ic dark:text-purple-500 text-purple-600">
                                <i class="fad fa-chevron-circle-right"></i>
                            </i>
                        </div>
                    </div>
                    <div style="animation-delay: .250s;" class="wipe_system animate__animated animate__fadeInUp ms-500 dark_border grid grid-cols-3 px-3 py-2 cursor-pointer rpl dark:bg-darkBlue-secondary rounded-lg shadow-lg">
                        <div class="flex flex-col col-span-2">
                            <p class="dark:text-gray-300 text-bluegray-900">Wipe System Data</p>
                        </div>
                        <div class="flex items-center flex-row justify-end">
                            <i class="settings_ic dark:text-purple-500 text-purple-600">
                                <i class="fad fa-chevron-circle-right"></i>
                            </i>
                        </div>
                    </div>
                </div>
                <div class="flex flex-col gap-2">
                    <div class="animate__animated animate__fadeInUp ms-500 flex items-center px-1">
                        <span class="dark:text-gray-300 text-base">Server Status</span>
                    </div>
                    <div style="animation-delay: .250s;" class="animate__animated animate__fadeInUp ms-500 dark_border grid grid-cols-3 px-3 py-2 cursor-pointer rpl dark:bg-darkBlue-secondary rounded-lg shadow-lg">
                        <div class="flex flex-col col-span-full">
                            <p class="dark:text-gray-300 text-bluegray-900">Operating System</p>
                            <p class="os dark:text-gray-400 text-bluegray-500 text-sm ml-1 mt-1">...</span>
                        </div>
                    </div>
                    <div style="animation-delay: .250s;" class="animate__animated animate__fadeInUp ms-500 dark_border grid grid-cols-3 px-3 py-2 cursor-pointer rpl dark:bg-darkBlue-secondary rounded-lg shadow-lg">
                        <div class="flex flex-col col-span-full">
                            <p class="dark:text-gray-300 text-bluegray-900">CPU</p>
                            <p class="cpu dark:text-gray-400 text-bluegray-500 text-sm ml-1 mt-1">...</span>
                        </div>
                    </div>
                    <div style="animation-delay: .250s;" class="animate__animated animate__fadeInUp ms-500 dark_border grid grid-cols-3 px-3 py-2 cursor-pointer rpl dark:bg-darkBlue-secondary rounded-lg shadow-lg">
                        <div class="flex flex-col col-span-full">
                            <p class="dark:text-gray-300 text-bluegray-900">Memory</p>
                            <p class="memory dark:text-gray-400 text-bluegray-500 text-sm ml-1 mt-1">...</span>
                        </div>
                    </div>
                    <div style="animation-delay: .250s;" class="animate__animated animate__fadeInUp ms-500 dark_border grid grid-cols-3 px-3 py-2 cursor-pointer rpl dark:bg-darkBlue-secondary rounded-lg shadow-lg">
                        <div class="flex flex-col col-span-full">
                            <p class="dark:text-gray-300 text-bluegray-900">Storage</p>
                            <p class="storage dark:text-gray-400 text-bluegray-500 text-sm ml-1 mt-1">...</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>