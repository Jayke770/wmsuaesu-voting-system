<% if(facial.image) { %> 
    <div class="container flex flex-col">
        <div class="container flex justify-center items-center py-2 px-2.5">
            <span class="dark:text-gray-300 text-xl">User Facial Data</span>
        </div>
        <div class="container flex justify-center items-center p-1">
            <img class="sm:w-full md:h-80 object-cover rounded-md shadow-md" src="<%- `data:image/jpeg;base64,${facial.image}` %>" />
        </div>
        <div class="container flex justify-center items-center">
            <% if(!facial.status) { %>
                <div class="w-1/3 sm:w-full grid grid-cols-2 gap-2 mt-4 pb-4 px-2">
                    <button data="<%- id %>" id="accept-facial" class="cursor-pointer dark:bg-teal-700 py-1.5 rounded-md dark:text-gray-200 w-full focus:outline-none focus:ring-2 focus:ring-offset-2 dark:focus:ring-offset-darkBlue-secondary focus:ring-teal-700 transition-all border-none" type="button">Accept</button>
                    <button data="<%- id %>" id="delete-facial" class="cursor-pointer dark:bg-rose-700 py-1.5 rounded-md dark:text-gray-200 w-full focus:outline-none focus:ring-2 focus:ring-offset-2 dark:focus:ring-offset-darkBlue-secondary focus:ring-rose-700 transition-all border-none" type="button">Delete Facial</button>
                </div>
            <% } else { %> 
                <div class="w-1/3 sm:w-full flex mt-4 pb-4 px-2">
                    <button data="<%- id %>" id="delete-facial" class="cursor-pointer dark:bg-rose-700 py-1.5 rounded-md dark:text-gray-200 w-full focus:outline-none focus:ring-2 focus:ring-offset-2 dark:focus:ring-offset-darkBlue-secondary focus:ring-rose-700 transition-all border-none" type="button">Delete Facial</button>
                </div>
            <% } %>
        </div>
    </div>
<% } else { %>
    <div class="container h-full flex justify-center items-center">
        <span class="text-xl dark:text-gray-200">No Facial Data</span>
    </div>
<% } %>